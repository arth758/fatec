package exercicioFixacao1;

public class JogoDaVelha {
	private Ocupacao[][] jogo = new Ocupacao[3][3];
	
	public enum Ocupacao {
		vazia(), jog1, jog2;
	}
	
	public JogoDaVelha() {
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				 this.jogo[i][j] = Ocupacao.vazia;
			}
		}
	}
}
