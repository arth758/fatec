package exerciciosFixacaoJava;

import static org.junit.Assert.*;
import org.junit.Test;

public class EmpregadoTest {
	@Test
	public void teste() {
		Empregado funcionario1 = new Empregado("Arthur", "Reis", 1600);
		Empregado funcionario2 = new Empregado("Marcos", "Alexandre", 1600);

		assertEquals("a", 1600 * 12, funcionario1.getSalarioAnual(), 0.01);
		assertEquals("b", 1600 * 12, funcionario2.getSalarioAnual(), 0.01);
	}
}
